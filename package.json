{
  "name": "bundlecomposer.json",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:web": "vite",
    "dev:api": "tsx watch src/server/index.ts",
    "dev": "concurrently -k -n API,WEB -c cyan,magenta \"pnpm dev:api\" \"pnpm dev:web\"",
    "gen:scenario": "tsx scripts/generate-scenario.ts --seed=demo --plans=8 --bundles=12 --channels=60 --outDir=src/server/mocks/fixtures",
    "gen:plans": "tsx scripts/generate-plans.ts --count=200 --seed=demo --out=src/server/mocks/fixtures/plans.base.json",
    "sanitize:plans": "tsx scripts/sanitize-plans.ts src/server/mocks/fixtures/plans.base.json --salt=v1",
    "regen:plans": "pnpm run gen:plans && pnpm run sanitize:plans",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@fastify/cors": "^9",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@reduxjs/toolkit": "^2.10.1",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/history": "^5.0.0",
    "axios": "^1.13.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fastify": "^4.29.1",
    "final-form": "^5.0.0",
    "final-form-arrays": "^4.0.0",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.552.0",
    "nanoid": "^5.1.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-final-form": "^7.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.9.5",
    "react-router-dom": "^7.9.5",
    "seedrandom": "^3.0.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.6",
    "commitlint": "^20.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "express": "^5.1.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "pino-pretty": "^13.1.2",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.3",
    "tailwindcss": "^4.1.17",
    "tsx": "^4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.6"
  }
}
